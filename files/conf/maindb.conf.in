{
	"log" : {
		"appender" : {
			"type" : "syslog"
		},
		"levels" : {
			"default" : "notice"
		}
	},
	"db" : {
		"path"	: "/var/db/main",
		"service_name" : "com.palm.db",
		"recoveryScriptPath" : "/usr/lib/db8/bin/errorOpenMainDb.bash",
		"permissions" : [
			{"type":"db.role","object":"admin","caller":"com.palm.configurator","operations":{"*":"allow"}},
			{"type":"db.role","object":"admin","caller":"com.palm.service.backup","operations":{"*":"allow"}},
			{"type":"db.role","object":"admin","caller":"com.palm.odd.service","operations":{"*":"allow"}},
			{"type":"db.role","object":"admin","caller":"com.palm.service.migrationscript","operations":{"*":"allow"}},
			{"type":"db.role","object":"admin","caller":"com.palm.spacecadet","operations":{"*":"allow"}}
		],
		"quotas" : [
                        {"owner":"*","size":10485760},
                        {"owner":"com.webos.*","size":94371840}

                ],
		"loadStepSize" : 173,
                "purgeWindow": 0,
                "canProfile" : @WANT_PROFILING@,
                "enableRootKind": true,
                "enablePurge": true
	},
	"bdb" : {
		"cacheSize": 3145728,
		"maxLocks" : 20000,
		"maxLockers" : 1000,
		"compactStepSize" : 25000
	},
	"sandwich" : {
		"cacheSize": 3145728,
		"maxLocks" : 20000,
		"maxLockers" : 1000,
		"compactStepSize" : 25000,
		"sync" : 1,
		"paranoid_checks" : 1,
		"verify_checksums" : 0,
		"fill_cache" : 1,
		"compression" : 0
	},
	"ldb" : {
		"cacheSize": 3145728,
		"maxLocks" : 20000,
		"maxLockers" : 1000,
		"compactStepSize" : 25000,
		"sync" : 1,
		"paranoid_checks" : 1,
		"verify_checksums" : 0,
		"fill_cache" : 1
	},
	"lmdb" : {
		"maxDBCount" : 6,
		"mapSize" : 1073741824,
		"writeMap" : 1,
		"noLock" : 0,
		"readOnly" : 0,
		"noMetaSync" : 0,
		"mapAsyc" : 0,
		"noMemInit" : 0,
		"noReadAhead" : 0,
		"noTls" : 1,
		"noSubDir" : 0,
		"fixedMap" : 0
	}
}
